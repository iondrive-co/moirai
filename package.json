{
  "name": "morai",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "node scripts/build-prod.js",
    "build:dev": "remix vite:build",
    "cf-typegen": "wrangler types",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "local": "npm run build:dev && wrangler pages dev --binding STORY_DATA --kv STORY_DATA --local",
    "start": "wrangler pages dev ./build/client",
    "story:get:prod": "node scripts/story-manager.js get-prod",
    "story:put:prod": "node scripts/story-manager.js put-prod",
    "story:load-example": "node scripts/story-manager.js load-example",
    "typecheck": "tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@reactflow/node-resizer": "^2.2.14",
    "@remix-run/cloudflare": "^2.13.1",
    "@remix-run/cloudflare-pages": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@eslint/config-array": "^0.19.1",
    "@eslint/object-schema": "^2.1.5",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@remix-run/dev": "^2.13.1",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^11.0.1",
    "postcss": "^8.4.38",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.103.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
